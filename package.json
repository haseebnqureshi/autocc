{
	"name": "autocc",
	"version": "1.3.0",
	"description": "Automated Claude Code workspace manager with intelligent branch naming and environment setup. Forked from ccmanager by Kodai Kabasawa.",
	"license": "MIT",
	"author": "Haseeb Qureshi",
	"contributors": [
		"Kodai Kabasawa (original ccmanager author)"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/haseebnqureshi/autocc.git"
	},
	"homepage": "https://github.com/haseebnqureshi/autocc#readme",
	"bugs": {
		"url": "https://github.com/haseebnqureshi/autocc/issues"
	},
	"keywords": [
		"claude",
		"code",
		"worktree",
		"git",
		"tui",
		"cli",
		"automation",
		"ai",
		"workflow",
		"branch-naming",
		"devtools"
	],
	"bin": {
		"autocc": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "node dist/cli.js",
		"test": "vitest --run",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "npm run lint && npm run typecheck && npm run build",
		"prepare": "npm run build"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@xterm/headless": "^5.5.0",
		"effect": "^3.18.2",
		"ink": "^5.1.3",
		"ink-select-input": "^6.2.0",
		"ink-text-input": "^6.0.0",
		"meow": "^11.0.0",
		"node-pty": "^1.0.0",
		"react": "^18.2.0",
		"strip-ansi": "^7.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.28.0",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/node": "^20.0.0",
		"@types/react": "^18.0.32",
		"@typescript-eslint/eslint-plugin": "^8.33.1",
		"@typescript-eslint/parser": "^8.33.1",
		"@vdemedes/prettier-config": "^2.0.1",
		"chalk": "^5.2.0",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^5.2.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^3.0.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"vitest": "^3.2.2"
	},
	"prettier": "@vdemedes/prettier-config"
}
